{
  "name": "Log",
  "plural": "logs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mysql": {
      "table": "todo_logs"
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "mysql": {
        "columnName": "id"
      }
    },
    "description": {
      "type": "string",
      "required": true,
      "length": 255,
      "mysql": {
        "columnName": "description"
      }
    },
    "date": {
      "type": "date",
      "required": true,
      "default": "$now",
      "mysql": {
        "columnName": "date_log"
      }
    },
    "taskId": {
      "type": "number",
      "id": true,
      "mysql": {
        "columnName": "for_task"
      }
    }
  },
  "mixins": {
    "ReadOnly" : {
      "id" : true,
      "date": true,
      "taskId": true
    }
  },
  "validations": [],
  "relations": {
    "task": {
      "type": "belongsTo",
      "model": "Task",
      "foreignKey": "taskId"
    }
  },
  "acls": [],
  "methods": {}
}
